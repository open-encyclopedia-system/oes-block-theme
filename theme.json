							"src": [
								"file:./assets/fonts/nexus-sans/NexusSans-Regular.otf"
							]
						},
						{
							"fontFamily": "Nexus Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/nexus-sans/NexusSans-Italic.otf"
							]
						},
						{
							"fontFamily": "Nexus Sans",
							"fontStretch": "normal",
							"fontStyle": "bold",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/nexus-sans/NexusSans-BoldSC.otf"
							]
						}
					],
					"fontFamily": "\"Nexus Sans\", sans-serif",
					"name": "Nexus Sans",
					"slug": "nexus-sans"
				},
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"min": ".8rem",
						"max": ".9rem"
					},
					"size": ".9rem",
					"slug": "small"
				},
				{
					"fluid": {
						"min": ".9rem",
						"max": "1rem"
					},
					"size": "1rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"min": "1.5rem",
						"max": "2rem"
					},
					"size": "1.5rem",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "2rem",
					"slug": "x-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true,
		"dimensions": {
			"minHeight": true
		},
		"position": {
			"sticky": true
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--text)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--background)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--text)",
						"text": "var(--wp--preset--color--text-invert)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--text)",
						"text": "var(--wp--preset--color--text-invert)"
					}
				},
				":active": {
					"color": {
						"background": "var(--wp--preset--color--text)",
						"text": "var(--wp--preset--color--text-invert)"
					}
				},
				":visited": {
					"color": {
						"text": "var(--wp--preset--color--text)"
					}
				}
			},
			"h1": {
				"typography": {
					"fontSize": "2rem"
				},
				"color": {
					"text": "var(--wp--preset--color--text)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "1.5rem"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "1.25rem"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "1.1rem"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "1rem",
					"textTransform": "uppercase"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "1rem",
					"textTransform": "uppercase"
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "400",
					"lineHeight": "1.4"
				}
			},
			"link": {
				"typography": {
					"textDecoration": "underline"
				},
				"color": {
					"text": "var(--wp--preset--color--text)"
				},
				"spacing": {
					"padding": {
						"top": "0",
						"right": "2px",
						"bottom": "0",
						"left": "2px"
					},
					"margin": {
						"top": "0",
						"bottom": "0",
						"left": "-2px"
					}
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--text)",
						"text": "var(--wp--preset--color--text-invert)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"background": "var(--wp--preset--color--text)",
						"text": "var(--wp--preset--color--text-invert)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"blocks": {
			"core/details": {
				"spacing": {
					"blockGap": "0"
				}
			}
		},
		"spacing": {
			"blockGap": "1em",
			"padding": {
				"top": "1rem",
				"right": "1rem",
				"bottom": "3rem",
				"left": "1rem"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--source-sans)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"lineHeight": "1.75"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "header",
			"name": "header_language1",
			"title": "Header (Language 1)"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "footer",
			"name": "footer_language1",
			"title": "Footer (Language 1)"
		},
		{
			"area": "uncategorized",
			"name": "title",
			"title": "Title"
		},
		{
			"area": "uncategorized",
			"name": "search-panel",
			"title": "Search Panel"
		}
	]
}
