{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "single-article",
			"title": "Single (Content)"
		},
		{
			"name": "single-contributor",
			"title": "Single (Contributor)"
		},
		{
			"name": "single-index",
			"title": "Single (Index)"
		},
		{
			"name": "archive-index",
			"title": "Archive (Index)"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#2a3130",
					"name": "Text",
					"slug": "text"
				},
				{
					"color": "#ffffff",
					"name": "Text Invert",
					"slug": "text-invert"
				},
				{
					"color": "#004589",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#b22222",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#337ab7",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#b3b3b3",
					"name": "Inactive",
					"slug": "inactive"
				},
				{
					"color": "#eee",
					"name": "Background",
					"slug": "background"
				}
			]
		},
		"layout": {
			"contentSize": "1000px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [
				"rem",
				"em",
				"%",
				"px",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-Black.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-BlackIt.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-Bold.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
