							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-ExtraLight.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-ExtraLightIt.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-It.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-Light.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-LightIt.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/source-sans-pro/SourceSansPro-Regular.otf"
							]
						},
						{
							"fontFamily": "Source Sans",
							"fontStretch": "normal",
